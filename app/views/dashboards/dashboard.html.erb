<div class="table-responsive col-md-6">
  <table class="table table-hover">
    <thead>
      <th class="col-md-4">カスタム取引情報</th>
      <th class="col-md-2 text-right">オーク数</th>
      <th class="col-md-2 text-right">非オーク数</th>
      <th class="col-md-2 text-right">&nbsp;</th>
      <th class="col-md-2 text-right">&nbsp;</th>
    </thead>
    <tbody>
    <tr>
      <th>今月</th>
      <td class="text-right">１１</td>
      <td class="text-right">２２</td>
      <td class="text-right">&nbsp;</td>
      <td class="text-right">&nbsp;</td>
    </tr>
    <tr>
      <th>今年</th>
      <td class="text-right">１１</td>
      <td class="text-right">２２</td>
      <td class="text-right">&nbsp;</td>
      <td class="text-right">&nbsp;</td>
    </tr>
    <tr>
      <th>全部</th>
      <td class="text-right">１１</td>
      <td class="text-right">２２</td>
      <td class="text-right">&nbsp;</td>
      <td class="text-right">&nbsp;</td>
    </tr>
    </tbody>
  </table>
