<div class="clearfix"></div>

<div class="table-responsive col-md-6">
  <table class="table table-hover">
    <thead>
      <th class="col-md-4">商品情報</th>
      <th class="col-md-2 text-right">国内</th>
      <th class="col-md-2 text-right">海外</th>
      <th class="col-md-4">&nbsp;</th>
    </thead>
    <tbody>
    <tr>
      <th class="col-md-4">未売数</th>
      <td class="col-md-2 text-right"><%= @summary["product"]["domesitc_unsale_cnt"] %></td>
      <td class="col-md-2 text-right"><%= @summary["product"]["offshore_unsale_cnt"] %></td>
      <td class="col-md-4">&nbsp;</td>
    </tr>
    <tr>
      <th class="col-md-4">売済数</th>
      <td class="col-md-2 text-right"><%= @summary["product"]["domesitc_sold_cnt"] %></td>
      <td class="col-md-2 text-right"><%= @summary["product"]["offshore_sold_cnt"] %></td>
      <td class="col-md-4">&nbsp;</td>
    </tr>
    <tr>
      <th class="col-md-4">発送中品</th>
      <td class="col-md-2 text-right">-</td>
      <td class="col-md-2 text-right"><%= @summary["product"]["offshore_sending_cnt"] %></td>
      <td class="col-md-4">&nbsp;</td>
    </tr>
    <tr>
      <th class="col-md-4">合計</th>
      <td class="col-md-2 text-right">
        <%= @summary["product"]["domesitc_unsale_cnt"].to_i + \
            @summary["product"]["domesitc_sold_cnt"].to_i %>
      </td>
      <td class="col-md-2 text-right">
        <%= @summary["product"]["offshore_unsale_cnt"].to_i + \
            @summary["product"]["offshore_sold_cnt"].to_i + \
            @summary["product"]["offshore_sending_cnt"].to_i %>
      </td>
      <td class="col-md-4">&nbsp;</td>
    </tr>
    </tbody>
  </table>
</div>

<div class="table-responsive col-md-4">
  <table class="table table-hover">
    <thead>
    <tr>
      <th class="col-md-6">未処置オーク情報</th>
      <th class="col-md-3 text-right">数</th>
      <th class="col-md-3">&nbsp;</th>
    </tr>
    </thead>
    <tbody>
    <tr>
      <th class="col-md-6">区分未登録</th>
      <td class="col-md-3 text-right"><%= @summary["product"]["domesitc_unsale_cnt"] %></td>
      <td class="col-md-3">&nbsp;</td>
    </tr>
    <tr>
      <th class="col-md-6">商品未登録</th>
      <td class="col-md-3 text-right"><%= @summary["product"]["domesitc_sold_cnt"] %></td>
      <td class="col-md-3">&nbsp;</td>
    </tr>
    <tr>
      <th class="col-md-6">カス未登録</th>
      <td class="col-md-3 text-right"><%= @summary["product"]["offshore_sending_cnt"] %></td>
      <td class="col-md-3">&nbsp;</td>
    </tr>
    <tr>
      <th class="col-md-6">合計</th>
      <td class="col-md-3 text-right">
        <%= @summary["product"]["offshore_unsale_cnt"].to_i + \
            @summary["product"]["offshore_sold_cnt"].to_i + \
            @summary["product"]["offshore_sending_cnt"].to_i %>
      </td>
      <td class="col-md-3">&nbsp;</td>
    </tr>
    </tbody>
  </table>
</div>

<div class="table-responsive col-md-2">
  <table class="table table-hover">
    <thead>
    <tr>
      <th colspan="2">未処置カスタム数</th>
    </tr>
    </thead>
    <tbody>
    <tr>
      <td class="col-md-6 text-right"><%= @summary["product"]["domesitc_unsale_cnt"] %></td>
      <td class="col-md-6">&nbsp;</td>
    </tr>
    </tbody>
  </table>
</div>
