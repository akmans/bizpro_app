<%= form_for(@auction) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <div class="formRow">
    <div class="formCell1"><%= f.label :auction_id, 'オークションID' %></div>
    <div class="formCell2"><%= f.text_field :auction_id, class: 'input' %></div>
  </div>

  <div class="formRow">
    <%= f.submit "実行", class: "btn btn-primary" %>
    <%= link_to "戻る", auctions_path, class: "btn" %>
  </div>
<% end %>
